	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>BalanceDue</Title>
                        <Shortcut></Shortcut>
			<Description>Adds Murach's Famous "BalanceDue" Expression</Description>
			<Author>kosmos434</Author>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal>
					<ID>Operator</ID>
					<ToolTip>Equal/less than/greater than</ToolTip>
					<Default>></Default>
				</Literal>

				<Literal>
					<ID>Value</ID>
					<ToolTip>100/0/Whatever</ToolTip>
					<Default>0</Default>
				</Literal>

				<!-- <Literal>
					<ID>SynonymName</ID>
					<ToolTip>Name of the synonym</ToolTip>
					<Default>SynonymName</Default>
				</Literal> -->
			</Declarations>
			<Code Language="SQL">
				<![CDATA[InvoiceTotal - PaymentTotal - CreditTotal $Operator$ $Value$]]>
			</Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>
